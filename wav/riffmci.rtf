\par \trowd \trgaph108\trleft612 \clbrdrt\brdrs\brdrw15 \clbrdrb\brdrs\brdrw15 \cellx2880\clbrdrt\brdrs\brdrw15 \clbrdrb\brdrs\brdrw15 \cellx8640\pard\plain \s13\sb120\intbl \fs20\lang1033 {\b Field}\cell {\b Description}\cell \pard\plain \intbl 
